{
  "name": "@viteworks/ts-no-duplicate",
  "private": false,
  "version": "0.0.4",
  "description": "TypeScript cross-file duplicate name detection tool",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "ts-no-duplicate": "dist/index.js"
  },
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "build": "tsdown",
    "dev": "tsx src/index.ts",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "update-deps": "pnpm update",
    "commit": "git-cz",
    "prepare": "husky"
  },
  "keywords": [
    "typescript",
    "ast",
    "duplicate",
    "detection"
  ],
  "author": "cola",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "glob": "^11.0.3",
    "jiti": "^2.5.1",
    "lodash-es": "^4.17.21",
    "ts-morph": "^26.0.0",
    "tsdown": "^0.14.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.34.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@types/glob": "^9.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.34.0",
    "git-cz": "^4.9.0",
    "globals": "^16.3.0",
    "husky": "^9.1.6",
    "release-it": "^19.0.4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
