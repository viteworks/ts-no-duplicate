{
  "name": "ts-no-duplicate",
  "version": "0.0.1",
  "description": "TypeScript cross-file duplicate name detection tool",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "ts-no-duplicate": "dist/index.js"
  },
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "build": "tsdown",
    "dev": "tsx src/index.ts",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "test:run": "vitest run --coverage",
    "test:watch": "vitest watch",
    "update-deps": "pnpm update",
    "commit": "git-cz",
    "prepare": "husky"
  },
  "keywords": [
    "typescript",
    "ast",
    "duplicate",
    "detection"
  ],
  "author": "",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "ts-morph": "^26.0.0",
    "tsdown": "^0.14.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "git-cz": "^4.9.0",
    "husky": "^9.1.6",
    "release-it": "^17.8.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
